{
  "name": "amitabha-pure-land",
  "version": "1.0.0",
  "description": "",
  "main": "amtf",
  "type": "module",
  "scripts": {
    "start": "node amtf",
    "postinstall": "ln -sf `pwd`/.parse* node_modules/f4amtf/web/ && ln -sf `pwd`/.env* node_modules/f4amtf/web/",
    "build": "cd node_modules/f4amtf/web && npm install && VUE_APP_PARENT_URL=.. VUE_APP_AMITABHA=香光庄严 VUE_APP_PUBLIC_PATH=/online/ npm run build -- --mode $NODE_ENV && mv dist ../online && ../scripts/post-build",
    "postbuild": "rm -rf online && mv node_modules/f4amtf/online .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint --cache \"**/*.{js,cjs}\"",
    "lint-fix": "eslint --cache --fix \"**/*.{js,cjs}\"",
    "prettier": "prettier --write '{cloud,lib}/{**/*,*}.js' 'amtf'",
    "watch": "babel-watch amtf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inflaton/amtf-proxy.git"
  },
  "author": "Inflaton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inflaton/amtf-proxy/issues"
  },
  "homepage": "https://github.com/inflaton/amtf-proxy#readme",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "amtf-proxy": "^1.2.9",
    "axios": "^0.21.4",
    "b4amtf": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "f4amtf": "^1.0.3",
    "node-cmd": "^5.0.0",
    "parse-server": "^4.5.0"
  },
  "devDependencies": {
    "babel-watch": "^7.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2"
  }
}
